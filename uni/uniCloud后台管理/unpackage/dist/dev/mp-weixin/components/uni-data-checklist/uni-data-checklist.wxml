<view class="uni-data-checklist"><block wx:if="{{multiple}}"><checkbox-group data-event-opts="{{[['change',[['chagne',['$event']]]]]}}" class="{{['checklist-group',(mode==='list')?'is-list':'',(wrap)?'is-wrap':'']}}" bindchange="__e"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label class="{{['checklist-box',item.labelClass]}}"><checkbox hidden="{{true}}" disabled="{{item.disable}}" value="{{item.value+''}}" checked="{{item.selected}}"></checkbox><block wx:if="{{mode!=='tag'}}"><view class="{{['checkbox__inner',item.checkboxBgClass]}}"><view class="{{['checkbox__inner-icon',item.checkboxClass]}}"></view></view></block><view class="checklist-content"><text class="{{['checklist-text',item.textClass]}}">{{item.text}}</text><block wx:if="{{mode==='list'}}"><view class="{{['checkobx__list',item.listClass]}}"></view></block></view></label></block></checkbox-group></block><block wx:else><radio-group data-event-opts="{{[['change',[['chagne',['$event']]]]]}}" class="{{['checklist-group',(mode==='list')?'is-list':'',(wrap)?'is-wrap':'']}}" bindchange="__e"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label class="{{['checklist-box',item.labelClass]}}"><radio hidden="{{true}}" disabled="{{item.disable}}" value="{{item.value+''}}" checked="{{item.selected}}"></radio><block wx:if="{{mode!=='tag'}}"><view class="{{['radio__inner',item.checkboxBgClass]}}"><view class="{{['radio__inner-icon',item.checkboxClass]}}"></view></view></block><view class="checklist-content"><text class="{{['checklist-text',item.textClass]}}">{{item.text}}</text><block wx:if="{{mode==='list'}}"><view class="{{['checkobx__list',item.listClass]}}"></view></block></view></label></block></radio-group></block></view>